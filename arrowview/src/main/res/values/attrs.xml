<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ArrowView">
        <attr name="av_lineColor" format="color"/>
        <attr name="av_direction" format="enum">
            <enum name="TOP" value="0"/>
            <enum name="LEFT" value="1"/>
            <enum name="RIGHT" value="2"/>
            <enum name="BOTTOM" value="3"/>
        </attr>
        <attr name="av_line_length" format="dimension"/>
        <attr name="av_line_width" format="dimension"/>
    </declare-styleable>
</resources>